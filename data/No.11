<h3 id="o083e6db">各レベル毎のステータス  <a class="anchor_super" href="https://grand_order.wicurio.com/index.php?cmd=edit&amp;page=%E3%82%A8%E3%83%9F%E3%83%A4&amp;id=o083e6db"><img src="//www.wicurio.com/pkwk/image/plus/paraedit.png" width="9" height="9" alt="Edit" title="Edit" /></a></h3>
<script type="text/javascript">
function switchRegionPart(prefix,page,regionId){
	var innerText = $('#rgn_content_'+prefix+'_'+regionId).text();
	innerText = innerText.replace(/\n/g,'');
	if( innerText == ''){
		var d = new Date();
		var ver = d.getSeconds();
		$('#rgn_content_'+prefix+'_'+regionId).prepend('<p id="rgn_lm_'+prefix+'_'+regionId+'">Loading...</p>');
		document.getElementById('rgn_summary_'+prefix+'_'+regionId).style.display='none';
		document.getElementById('rgn_content_'+prefix+'_'+regionId).style.display='block';
		document.getElementById('rgn_bracket_'+prefix+'_'+regionId).style.borderStyle='solid none solid solid';
		document.getElementById('rgn_button_'+prefix+'_'+regionId).innerHTML='-';
		$.ajax('https://grand_order.wicurio.com/index.php?plugin=region&target_page='+page+'&regionId='+regionId+'&ver='+ver,{dataType:'json',async: true})
		.done(function(data){
			showFetchedRegionContents(prefix,regionId,data);
		})
		.fail(function() {
			window.alert('正しい結果を得られませんでした。');
		});
	}else{
		if(document.getElementById('rgn_summary_'+prefix+'_'+regionId).style.display!='none'){
			document.getElementById('rgn_summary_'+prefix+'_'+regionId).style.display='none';
			document.getElementById('rgn_content_'+prefix+'_'+regionId).style.display='block';
			document.getElementById('rgn_bracket_'+prefix+'_'+regionId).style.borderStyle='solid none solid solid';
			document.getElementById('rgn_button_'+prefix+'_'+regionId).innerHTML='-';
		}else{
			document.getElementById('rgn_summary_'+prefix+'_'+regionId).style.display='block';
			document.getElementById('rgn_content_'+prefix+'_'+regionId).style.display='none';
			document.getElementById('rgn_bracket_'+prefix+'_'+regionId).style.borderStyle='none';
			document.getElementById('rgn_button_'+prefix+'_'+regionId).innerHTML='+';
		}
	}
}
function showFetchedRegionContents(prefix,regionId,body){
	$('#rgn_lm_'+prefix+'_'+regionId).remove();
	var region_body = body.body;
	$('#rgn_content_'+prefix+'_'+regionId).prepend(region_body);
	if(body.script_tag.length > 0){
		for(var i=0;i<body.script_tag.length;i++){
			var js_obj = body.script_tag[i];
			var script = document.createElement('script');
			script.type = 'text/javascript';
			if(js_obj.src != ''){
				script.src = js_obj.src;
			}
			document.getElementById('rgn_content_'+prefix+'_'+regionId).appendChild(script);
		}
	}
}
